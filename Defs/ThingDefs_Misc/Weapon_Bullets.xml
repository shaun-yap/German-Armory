<?xml version="1.0" encoding="utf-8" ?> 

<Defs>
    <!-- Base German Armory Bullet -->
    <ThingDef Name="BaseBulletGA" Abstract="True" ParentName="BaseBullet">
        <defName>BaseBulletGA</defName>
        <label>Base GA Bullet</label>
        <graphicData>
            <texPath>Things/Projectile/Bullet_small</texPath>
