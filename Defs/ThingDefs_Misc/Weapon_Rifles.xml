            <texPath>Weapon/BoltAction/Gew_98</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>2.00</drawSize>
        </graphicData>
        <soundInteract>Interact_Rifle</soundInteract>
        <recipeMaker>
            <researchPrerequisite>GAStage0</researchPrerequisite>
            <skillRequirements>
                <Crafting>3</Crafting>
            </skillRequirements>
        </recipeMaker>
